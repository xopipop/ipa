{
  "version": 2,
  "outputDirectory": "public",
  "headers": [
    {
      "source": "/app.ipa",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"app.ipa\""
        }
      ]
    },
    {
      "source": "/manifest.plist",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/xml"
        }
      ]
    },
    {
      "source": "/icon.svg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
